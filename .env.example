# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:pass@ep-xxx.region.neon.tech/career_builder?sslmode=require

# Cache (Upstash Redis)
UPSTASH_REDIS_REST_URL=https://xxx.upstash.io
UPSTASH_REDIS_REST_TOKEN=xxx

# AI (OpenAI)
OPENAI_API_KEY=sk-xxx

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# NextAuth
NEXTAUTH_SECRET=your-secret-key-here-generate-with-openssl-rand-base64-32
NEXTAUTH_URL=http://localhost:3000

# OAuth - Google
GOOGLE_CLIENT_ID=xxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=xxx

# OAuth - Twitter/X
TWITTER_CLIENT_ID=xxx
TWITTER_CLIENT_SECRET=xxx

# OAuth - WeChat (optional - login button hidden if not set)
# Get credentials from https://open.weixin.qq.com/
WECHAT_APP_ID=xxx
WECHAT_APP_SECRET=xxx

# Web3 - WalletConnect
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=xxx

# Web Search - Tavily (optional - web search for trending tech disabled if not set)
# Get API key from https://tavily.com/
TAVILY_API_KEY=tvly-xxx
